    <div class="">
        <a class="hiddenanchor" id="toregister"></a>
        <a class="hiddenanchor" id="tologin"></a>

        <div id="wrapper">                        
            @Html.Partial("toLogin")
            @Html.Partial("toRegister")
        </div>
    </div>

</body>

</html>
